---
interface Props {
path: string;
url: string;
}
const {path,url} = Astro.props;
---
<div class="flex ">
 <a href={url} class="flex rounded-full bg-white opacity-90 h-10 w-10 p-2 md:h-[3.2rem] md:w-[3.2rem] md:mt-52 xl:mt-64  2xl:mt-[26rem] 2xl:h-20 2xl:w-20 place-content-center"> 
   <img src={path} alt="RedSocial" />
  </img>
</img>